{
    "name": "bryntum-gantt-theme-triton",
    "sencha": {
        "namespace": "",
        "format": "1",
        "version": "6.1.15",
        "compatVersion": "6.1.15",
        "creator": "Bryntum",
        "summary": "The Triton theme for Ext Gantt",
        "detailedDescription": "The Triton theme for Ext Gantt",
        "type": "theme",
        "extend": "bryntum-scheduler-theme-triton",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "toolkit": "classic",
        "output": "${package.dir}/build",
        "sass": {
            "namespace": "",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "framework": "ext",
        "requires": [
            "bryntum-gantt-theme-base"
        ],
        "builds": {
            "pro": {
                "output": "${package.dir}/build/pro",
                "requires": [
                    "bryntum-gantt-pro@6+",
                    "bryntum-gantt-theme-base"
                ]
            },
            "standard": {
                "output": "${package.dir}/build/standard",
                "requires": [
                    "bryntum-gantt@6+",
                    "bryntum-gantt-theme-base"
                ]
            }
        }
    }
}
