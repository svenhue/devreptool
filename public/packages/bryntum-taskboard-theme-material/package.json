{
    "name"   : "bryntum-taskboard-theme-material",
    "sencha" : {
        "namespace"           : "",
        "format"              : "1",
        "version"             : "2.0.30",
        "compatVersion"       : "2.0.30",
        "creator"             : "Bryntum",
        "summary"             : "The Material theme for Taskboard",
        "detailedDescription" : "The Material theme for Taskboard",
        "type"                : "theme",
        "extend"              : "theme-material",
        "slicer"              : {
            "js" : [
                {
                    "path"             : "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest" : true
                }
            ]
        },
        "toolkit"             : "classic",
        "output"              : "${package.dir}/build",
        "sass"                : {
            "namespace" : "",
            "etc"       : "${package.dir}/sass/etc/all.scss",
            "var"       : "${package.dir}/sass/var",
            "src"       : "${package.dir}/sass/src"
        },
        "classpath"           : "${package.dir}/src",
        "overrides"           : "${package.dir}/overrides",
        "framework"           : "ext",
        "requires"            : [
            "bryntum-taskboard",
            "bryntum-taskboard-theme-base",
            "font-ext",
            "font-awesome"
        ]
    }
}
