{
    "name": "bryntum-scheduler-theme-triton",
    "sencha": {
        "namespace": "",
        "format": "1",
        "version": "6.1.15",
        "compatVersion": "6.1.15",
        "creator": "Bryntum",
        "summary": "The Triton theme for Ext Scheduler",
        "detailedDescription": "The Triton theme for Ext Scheduler",
        "type": "theme",
        "extend": "theme-triton",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "toolkit": "classic",
        "output": "${package.dir}/build",
        "sass": {
            "namespace": "",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": "${package.dir}/sass/src"
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "framework": "ext",
        "requires": [
            "bryntum-scheduler@6+",
            "bryntum-scheduler-theme-base",
            "font-awesome"
        ]
    }
}
